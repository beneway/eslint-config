  "devDependencies": {
    "@benewy/eslint-config": "*",
    "eslint-plugin-benewy": "workspace:*",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
